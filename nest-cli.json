{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/social-network/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/social-network/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/social-network",
  "projects": {
    "social-network": {
      "type": "application",
      "root": "apps/social-network",
      "entryFile": "main",
      "sourceRoot": "apps/social-network/src",
      "compilerOptions": {
        "tsConfigPath": "apps/social-network/tsconfig.app.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "authentication-service": {
      "type": "application",
      "root": "apps/authentication-service",
      "entryFile": "main",
      "sourceRoot": "apps/authentication-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authentication-service/tsconfig.app.json"
      }
    },
    "users-service": {
      "type": "application",
      "root": "apps/users-service",
      "entryFile": "main",
      "sourceRoot": "apps/users-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users-service/tsconfig.app.json"
      }
    },
    "feed-service": {
      "type": "application",
      "root": "apps/feed-service",
      "entryFile": "main",
      "sourceRoot": "apps/feed-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/feed-service/tsconfig.app.json"
      }
    },
    "notification-service": {
      "type": "application",
      "root": "apps/notification-service",
      "entryFile": "main",
      "sourceRoot": "apps/notification-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-service/tsconfig.app.json"
      }
    }
  }
}